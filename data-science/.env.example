# Database Configuration (matches root .env)
# These are automatically loaded in Airflow - no need to copy manually

# Enable database writes in pipeline
CALCUTTA_WRITE_TO_DB=true

# Database Connection (same as main app)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=calcutta
DB_USER=calcutta
DB_PASSWORD=

# Connection Pool Settings
DB_MIN_CONN=1
DB_MAX_CONN=10

# For Docker/Airflow, use service name
# DB_HOST=db
